gin_y\r\n    }\r\n\r\n    const sin = Math.sin(theta)\r\n    const cos = Math.cos(theta)\r\n\r\n    const m = a * cos - b * sin\r\n    const n = a * sin - b * cos\r\n\r\n    const len =Math.sqrt((Math.pow((origin_x - x_axis_x), 2) + Math.pow((origin_y - x_axis_y), 2)))\r\n\r\n    return {\r\n        m_ratio : m / len,\r\n        n_ratio : n / len\r\n    }\r\n}","C:\\Users\\prath\\OneDrive\\Desktop\\guitar-app\\src\\Utils\\CheckParallel.js",["160"],"export default (corners) => {\r\n    const len1 = Math.pow(corners[0].x - corners[1].x, 2) + Math.pow(corners[0].y - corners[1].y, 2)\r\n    const len2 = Math.pow(corners[1].x - corners[2].x, 2) + Math.pow(corners[1].y - corners[2].y, 2)\r\n    \r\n    return Math.abs((len1 - len2)) < 500\r\n}","C:\\Users\\prath\\OneDrive\\Desktop\\guitar-app\\src\\Utils\\Map.js",[],"C:\\Users\\prath\\OneDrive\\Desktop\\guitar-app\\src\\Utils\\Song.js",["161"],"const Song = (s) => {\r\n    const LYRICS = 'LYRICS'\r\n    const TAB = 'TAB'\r\n    \r\n    const list = []\r\n\r\n    const getNext = (pos) => {\r\n        pos += 5;\r\n        var tab = \"\"\r\n        var n = s.length;\r\n        while(pos < n){\r\n            if(s.charAt(pos) == '<')\r\n                break;\r\n            tab += s.charAt(pos)\r\n            pos++;\r\n        }\r\n        return {\r\n            pos : pos + 5,\r\n            tab : tab\r\n        }\r\n    }\r\n\r\n    var text = \"\"\r\n    for (var i = 0; i < s.length; i++) {\r\n        var c = s.charAt(i);\r\n        if(c === '<'){\r\n            if(text !==\"\")\r\n                list.push({\r\n                    data: text,\r\n                    type: LYRICS\r\n                })\r\n            var tab;\r\n            var res = getNext(i);\r\n            tab = res.tab\r\n            i = res.pos\r\n            text = \"\"\r\n            list.push({\r\n                type: TAB,\r\n                data: tab\r\n            })\r\n        } else\r\n            text += s.charAt(i)\r\n    }\r\n\r\n    if(text 